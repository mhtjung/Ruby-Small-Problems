  
<div id="content">
  <h2>Details for <%= @current_user.to_s.capitalize %></h2>
  <h4>Email Address: <%= @userdata[@current_user][:email] %></h4>
  <h4>Interests: <%= @userdata[@current_user][:interests].join(', ') %></h4>
</div>

<div id="bottom_menu">
  <ul>
    <% @userdata.each do |user, info| %>
      <% next if user == @current_user %>
      <li>
        <a href="/users/<%= user %>"><%= user %></a>
      </li>
    <% end %>
  </ul>
</div>